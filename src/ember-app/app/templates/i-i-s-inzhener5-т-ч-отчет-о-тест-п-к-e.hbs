<h3 class="ui header">{{t "forms.i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.тестируемые_приложения_и_игры
      class=(if (v-get validationModel "тестируемые_приложения_и_игры" "isInvalid") "error")
      label=(t "forms.i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e.тестируемые_приложения_и_игры-caption")
      data-test-i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e-тестируемые_приложения_и_игры=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тестируемые_приложения_и_игры" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.операционная_система
      class=(if (v-get validationModel "операционная_система" "isInvalid") "error")
      label=(t "forms.i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e.операционная_система-caption")
      data-test-i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e-операционная_система=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "операционная_система" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.результаты_тестирования
      class=(if (v-get validationModel "результаты_тестирования" "isInvalid") "error")
      label=(t "forms.i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e.результаты_тестирования-caption")
      data-test-i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e-результаты_тестирования=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "результаты_тестирования" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.выводы_и_рекомендации
      class=(if (v-get validationModel "выводы_и_рекомендации" "isInvalid") "error")
      label=(t "forms.i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e.выводы_и_рекомендации-caption")
      data-test-i-i-s-inzhener5-т-ч-отчет-о-тест-п-к-e-выводы_и_рекомендации=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "выводы_и_рекомендации" "message")}}
  </div>
</form>
